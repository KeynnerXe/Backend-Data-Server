generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ventas {
  id       Int       @id @default(autoincrement())
  producto String?   @db.VarChar(50)
  cantidad Int?
  precio   Decimal?  @db.Decimal
  fecha    DateTime? @db.Date
}
